{
  "name": "node-biolareplat",
  "version": "1.0.0",
  "description": "This repository is for Proximity Social App.",
  "main": "build/server.js",
  "scripts": {
    "format:check": "prettier --check \"./src/**/*.ts\"",
    "format:write": "prettier --write \"./src/**/*.ts\"",
    "lint:check": "eslint \"./src/**/*.ts\"",
    "lint:fix": "eslint --fix .",
    "dev:tsc": "tsc -w",
    "dev:node": "nodemon build/server.js",
    "dev": "npm run format:check && npm run lint:check && concurrently npm:dev:*",
    "start": "npm run db:migration && node build/server.js",
    "build1": "npm run lint:check && bash -c tsc -p tsconfig.json --noEmit",
    "build:prod": "npm run lint:check && tsc -p tsconfig.json && npm run copy:lib",
    "build": "tsc",
    "copy:lib": "xcopy lib build\\lib /E /I /Y",
    "db:migration": "npx sequelize-cli db:migrate",
    "db:create": "npx sequelize-cli db:create",
    "db:migration:undo": "npx sequelize-cli db:migrate:undo",
    "db:migration:generate": "sequelize migration:generate --migrations-path src/db/migrations --name"
  },
  "author": "smartSense",
  "license": "ISC",
  "dependencies": {
    "address": "^2.0.3",
    "async": "^3.2.2",
    "atob": "^2.1.2",
    "aws-sdk": "^2.1659.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "binary-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.1",
    "express-fileupload": "^1.5.0",
    "express-validator": "^7.3.0",
    "firebase-admin": "^13.4.0",
    "handlebars": "^4.7.8",
    "http-status-codes": "^2.3.0",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.1",
    "mustache": "^4.2.0",
    "nodemailer": "^7.0.10",
    "pako": "^2.1.0",
    "pg": "^8.12.0",
    "public-ip": "^8.0.0",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.11.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@types/async": "^3.2.12",
    "@types/atob": "^2.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/binary-parser": "^1.5.5",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.0",
    "@types/express-fileupload": "^1.5.0",
    "@types/i18n": "^0.13.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mustache": "^4.2.5",
    "@types/nodemailer": "^6.4.15",
    "@types/pako": "^2.0.3",
    "@types/sequelize": "^4.28.14",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.3",
    "prettier": "^3.0.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "npm": "10.7.0",
    "node": "20.15.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run build",
      "git add --all"
    ]
  },
  "hooks": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  }
}
